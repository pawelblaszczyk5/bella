{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"import": ["@cspell/dict-pl_pl/cspell-ext.json"],
	"ignorePaths": [
		"pnpm-lock.yaml",
		"cspell.json",
		"**.svg",
		".git/",
		".gitignore",
		".vscode/",
		"LICENSE.md",
		"tooling/environment/miscellaneous/grafana-dashboard.json"
	],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"codeowner",
		"electricsql",
		"extralight",
		"lingui",
		"millis",
		"oklch",
		"rolldown",
		"spritesheet",
		"stylex",
		"stylexjs",
		"tanstack",
		"timestamptz",
		"typecheck",
		"vite",
		"vitejs"
	],
	"enableGlobDot": true,
	"ignoreWords": ["caddyfile", "pawelblaszczyk", "predev", "taze", "tseslint"]
}
